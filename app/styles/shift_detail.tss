},
".timeSet":{
	width:Ti.UI.FILL,
	height:Ti.UI.SIZE,
	border:Ti.UI.INPUT_BORDERSTYLE_ROUNDED,
	bottom:'10',
	backgroundColor:'#fff',
	borderRadius:10,
	borderColor:'#fff',
	height:'40dp'
},
".delete":{
	height:'25dp',
	right:'5dp',
	width:'55dp',
	color:'#fff',
	backgroundColor:'#aeaeae',
	textAlign:'center',
	border:Ti.UI.INPUT_BORDERSTYLE_ROUNDED,
	borderRadius:10,
	zIndex:5,
	visible:false
